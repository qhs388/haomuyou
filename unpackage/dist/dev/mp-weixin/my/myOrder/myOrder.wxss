page {
  background-color: #f5f5f5;
}
image {
  width: 100%;
  height: 100%;
}
.header {
  height: 286rpx;
  background-color: #ff1f44;
  padding: 25rpx;
  padding-top: 0rpx;
  box-sizing: border-box;
}
.header .picTxt {
  width: 100%;
  height: 208rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.header .picTxt .text {
  color: #FFFFFF;
  font-size: 26rpx;
  font-family: GuildfordProBook\ 5;
}
.header .picTxt .text .name {
  font-size: 34rpx;
  font-weight: 700;
  margin-bottom: 20rpx;
}
.header .picTxt .pictrue {
  width: 134rpx;
  height: 120rpx;
}
.header .nav {
  width: 99%;
  margin: 0 auto;
}
.list {
  overflow: hidden;
  padding: 25rpx;
  box-sizing: border-box;
  margin-top: 25rpx;
}
.list .item {
  background-color: #FFFFFF;
  border-radius: 10rpx;
  margin-bottom: 10rpx;
}
.list .item .title {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 20rpx 25rpx;
  box-sizing: border-box;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  color: #282828;
  font-size: 30rpx;
  border-bottom: 1rpx solid #eee;
}
.list .item .title .status {
  color: #E34819;
}
.list .item .shops {
  overflow: hidden;
  padding: 20rpx 25rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #eee;
}
.list .item .shops .shop {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  font-size: 28rpx;
  margin-bottom: 10rpx;
}
.list .item .shops .shop .shop-left {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.list .item .shops .shop .shop-left .pictrue {
  width: 132rpx;
  height: 132rpx;
  border-radius: 15rpx;
}
.list .item .shops .shop .shop-left .name {
  width: 340rpx;
  color: #282828;
  margin-left: 15rpx;
}
.list .item .shops .shop .text {
  color: #999;
  text-align: right;
  height: 132rpx;
  position: relative;
}
.list .item .shops .shop .text .num {
  position: absolute;
  bottom: 0;
  right: 0;
}
.list .item .shops .totalPrice {
  font-size: 26rpx;
  color: #282828;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: end;
  -webkit-justify-content: flex-end;
          justify-content: flex-end;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin-top: 20rpx;
}
.list .item .shops .totalPrice .money {
  margin-left: 10rpx;
  color: #E34819;
}
.list .item .bottom {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: end;
  -webkit-justify-content: flex-end;
          justify-content: flex-end;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 20rpx 25rpx;
  box-sizing: border-box;
}
.not-list {
  width: 440rpx;
  height: 330rpx;
  margin: 0 auto;
  margin-top: 150rpx;
}

