page {
  background-color: #f5f5f5;
}
image {
  width: 100%;
  height: 100%;
  display: block;
}
.content {
  position: relative;
  background-color: #fff;
}
.content .line {
  width: 100%;
  height: 4rpx;
  position: absolute;
  top: 0rpx;
}
.content .item {
  padding: 25rpx;
  box-sizing: border-box;
}
.content .item .item-top {
  padding-bottom: 20rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #eee;
}
.content .item .item-top .item-view {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 28rpx;
  color: #282828;
  font-weight: 700;
}
.content .item .item-top .item-view .phone {
  margin-left: 20rpx;
}
.content .item .item-top .address {
  margin-top: 15rpx;
  font-size: 28rpx;
}
.content .item .item-bottom {
  padding-top: 20rpx;
  box-sizing: border-box;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.content .item .item-bottom .bottom-left {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.content .item .item-bottom .bottom-left .text {
  font-size: 28rpx;
  color: #282828;
  margin-left: 16rpx;
}
.content .item .item-bottom .margin-right {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.content .item .item-bottom .margin-right .right-item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.content .item .item-bottom .margin-right .right-item .icon {
  width: 35rpx;
  height: 35rpx;
}
.content .item .item-bottom .margin-right .right-item .text {
  font-size: 28rpx;
  color: #2c2c2c;
  margin-left: 10rpx;
}
.foot {
  width: 100%;
  background-color: #FFFFFF;
  position: fixed;
  bottom: 0rpx;
  overflow: hidden;
  z-index: 10;
}
.foot .tips {
  width: 100%;
  padding: 12rpx;
  box-sizing: border-box;
  background-color: #FFF9F2;
  font-size: 22rpx;
  color: #FF6941;
  text-align: center;
}
.foot .button {
  padding: 16rpx 24rpx;
  box-sizing: border-box;
}

